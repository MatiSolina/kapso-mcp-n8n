{
    "name": "n8n-nodes-kapso",
    "version": "0.1.2",
    "description": "n8n community node for Kapso AI - WhatsApp Cloud API integration",
    "license": "MIT",
    "homepage": "https://kapso.ai",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "kapso",
        "whatsapp",
        "whatsapp-cloud-api"
    ],
    "author": {
        "name": "Kapso AI",
        "email": "support@kapso.ai"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MatiSolina/kapso-mcp-n8n.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/KapsoApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Kapso/Kapso.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "n8n-workflow": "^2.0.1",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
